<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="135"
                android:centerColor="@color/card_content"
                android:endColor="@color/card_content"
                android:startColor="@color/card_content"
                android:type="linear"/>
            <stroke
                android:width="1dp"
                android:color="@color/card_border"/>

            <corners
                android:radius="1dp"/>

        </shape>
    </item>
</layer-list>